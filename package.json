{
  "name": "@umbriel/components",
  "private": false,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prepare": "npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "peerDependencies": {
    "react": "^16.8.0 || 17.x",
    "react-dom": "^16.8.0 || 17.x",
    "styled-components": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Umbriel-CMS/umbriel-stories.git"
  },
  "main": "./dist/@umbriel-components.umd.js",
  "module": "./dist/@umbriel-components.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/@umbriel-components.es.js",
      "require": "./dist/@umbriel-components.umd.js"
    }
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Umbriel-CMS/umbriel-stories/issues"
  },
  "homepage": "https://github.com/Umbriel-CMS/umbriel-stories#readme"
}
